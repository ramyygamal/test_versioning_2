SELECT product_process_platform,description
FROM (
SELECT product_process_platform,description,
ROW_NUMBER() OVER(PARTITION BY id ORDER BY sync_time DESC ) AS row_num,
FROM
${ref("MP_GETVIEW_ProductProcessPlatforms_004")}
)
WHERE
row_num =1